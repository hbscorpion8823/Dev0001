#:kivy 1.0.9

FloatLayout:
    canvas.before:
        Color:
            rgba: 0, 0.5, 0.8, 1
        Rectangle:
            # self here refers to the widget i.e FloatLayout
            pos: self.pos
            size: self.size
    Button:
        text: 'Hello World!!'
        size_hint: None, None
        size: '300dp', '200dp'
        pos_hint: {'center_x':.9, 'center_y': .9}