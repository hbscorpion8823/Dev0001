#:kivy 1.0.9

#:include force other.kv

<MainGame>:
    layout01: l01

    FloatLayout:
        size: root.size
        id: l01
        canvas.before:
            Color:
                rgba: 0, 0.5, 0.8, 1
            Rectangle:
                # self here refers to the widget i.e FloatLayout
                pos: self.pos
                size: self.size

<MyImages>:
    source: 'images001.png'
    size: self.texture_size

