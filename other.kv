#:kivy 1.0.9


<ExplosionImage>:
    source: 'imgs/explode.zip'
    anim_delay: 0.05
    size_hint: None, None

<BaseObj>:
    lifePoint: 1
    g: 0
    size_hint: None, None
    canvas:
        Color: 
            rgba: 1., 1., 1., 1.
        Rectangle:
            texture: self.texture
            pos: self.pos
            size: self.size

